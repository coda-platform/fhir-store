{
    "appName": "fhir-store",
    "instanceCount": 1,
    "notExposeAsWebApp": false,
    "containerHttpPort": "$AIDBOX_PORT",
    "forceSsl": true,
    "envVars": [
        {
            "key": "AIDBOX_PORT",
            "value": "$AIDBOX_PORT"
        },
        {
            "key": "AIDBOX_LICENSE_ID",
            "value": "$AIDBOX_LICENSE_ID"
        },
        {
            "key": "AIDBOX_LICENSE_KEY",
            "value": "$AIDBOX_LICENSE_KEY"
        },
        {
            "key": "AIDBOX_FHIR_VERSION",
            "value": "$AIDBOX_FHIR_VERSION"
        },
        {
            "key": "AIDBOX_CLIENT_ID",
            "value": "$AIDBOX_CLIENT_ID"
        },
        {
            "key": "AIDBOX_CLIENT_SECRET",
            "value": "$AIDBOX_CLIENT_SECRET"
        },
        {
            "key": "PGHOST",
            "value": "$PGHOST"
        },
        {
            "key": "PGPORT",
            "value": "$PGPORT"
        },
        {
            "key": "PGUSER",
            "value": "$PGUSER"
        },
        {
            "key": "PGPASSWORD",
            "value": "$PGPASSWORD"
        },
        {
            "key": "PGDATABASE",
            "value": "$PGDATABASE"
        }
    ]
}